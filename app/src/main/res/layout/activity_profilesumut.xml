<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="@drawable/bgsc"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".profilesumut">

    <ImageView
        android:id="@+id/prov"
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:src="@drawable/prov" />
    <ImageButton
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:src="@drawable/backbutton"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="25dp"/>

    <ImageButton
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_alignParentRight="true"
        android:layout_marginTop="25dp"
        android:layout_marginRight="10dp"
        android:src="@drawable/sharebutton" />

    <ImageView
        android:id="@+id/sumutlogo"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/prov"
        android:src="@drawable/logosumut" />

    <ImageView
        android:id="@+id/sumuttext"
        android:layout_width="match_parent"
        android:layout_height="150dp"
        android:layout_below="@+id/sumutlogo"
        android:layout_marginTop="15dp"
        android:src="@drawable/sumuttextt" />

    <ImageButton
        android:id="@+id/btnRmh"
        android:layout_width="130dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/sumuttext"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="15dp"
        android:src="@drawable/btnrmh" />

    <ImageButton
        android:layout_width="130dp"
        android:layout_centerHorizontal="true"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:layout_below="@id/btnRmh"
        android:src="@drawable/btnbaju"
        android:id="@+id/btnbaju"/>

    <ImageButton
    android:layout_width="130dp"
    android:layout_centerHorizontal="true"
    android:layout_height="wrap_content"
    android:layout_marginTop="10dp"
    android:layout_below="@id/btnbaju"
    android:src="@drawable/btnalat"
    android:id="@+id/btnalat"/>


    <ImageView
        android:layout_width="match_parent"
        android:layout_height="68dp"
        android:layout_marginTop="685dp"
        android:src="@drawable/rectanglee" />

    <ImageButton
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="685dp"
        android:src="@drawable/search" />

    <ImageButton
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="685dp"
        android:src="@drawable/home" />

    <ImageButton
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginTop="685dp"
        android:layout_marginRight="20dp"
        android:layout_alignParentRight="true"
        android:src="@drawable/questionmark"/>




</RelativeLayout>